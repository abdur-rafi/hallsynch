////////  Mutations used for testing(will help to create api documentation)  ////////


mutation {
  addOldMealItem(name: "curry", type: "non-veg", cupCount: 15, mealTime: "lunch", date: "29 july, 2023")
  {
    cupcount,
    itemId,
    item {
      name,
      type,
      meals{
        mealId
      }
    },
    mealPlan {
      mealPlanId,
      day,
      mealTime,
      meal {
        createdAt
      }
    }
  }
}


mutation {
  addNewMealItem(name: "curry", type: "non-veg", cupCount: 30, mealTime: "lunch", date: "27 july, 2023", photoLocation: "")
  {
    cupcount,
    itemId,
    item {
      name,
      type,
      photo {
        filePath
      },
      meals{
        mealId
      }
    },
    mealPlan {
      mealPlanId,
      day,
      mealTime,
      meal {
        createdAt,
        items {
          itemId
        }
      }
    }
  }
}